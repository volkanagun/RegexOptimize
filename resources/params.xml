<?xml version="1.0" encoding="utf-8"?>
<ROOT>
<PARAMETERS>
<PARAM NAME="MAX_COMBINE_SIZE" VALUE="15"/>
<!--max regex size is the final regex size for single attribute value-->
<PARAM NAME="MAX_REGEX_SIZE" VALUE="40"/>
<!--max paths is equal to maxCombineSize * maxRegexSize-->
<PARAM NAME="MAX_PATHS" VALUE="600"/>
<PARAM NAME="MAX_MULTI_DEPTH" VALUE="3"/>
<PARAM NAME="MAX_SAMPLES" VALUE="5000"/>
<PARAM NAME="FOLD_SIZE" VALUE="10"/>
<PARAM NAME="NGRAM_LENGTH" VALUE="5"/>
<PARAM NAME="NGRAM_STEP_LENGTH" VALUE="1"/>
<!--top count is equal to input sample size-->
<PARAM NAME="TOP_COUNT" VALUE="50"/>
<PARAM NAME="PATTERN_FILTER_RATIO" VALUE="0.0"/>
<PARAM NAME="MATCH_SELECT_RATIO" VALUE="0.01"/>
<PARAM NAME="EXPERIMENT_CYCLE" VALUE="SINGLE-EXACT++REGEX-SINGLE"/>
<PARAM NAME="SELECTED_DOMAINS" VALUE=""/>
<PARAM NAME="NGRAM_FILTER" VALUE="true"/>
<PARAM NAME="DOMAIN_EXPERIMENT" VALUE="true"/>
<PARAM NAME="MIN_POS_SAMPLES" VALUE="1000"/>
<PARAM NAME="RND_GROUPING_SIZE" VALUE="3"/>
</PARAMETERS>
</ROOT>
